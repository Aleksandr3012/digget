mixin footer()
	+b.FOOTER.footer.block-with-lazy
		+e.main
			.container
				.row
					.col-lg-auto
						+e.logo(href="/")
							+pic('logo-white.png','','')
						+e.location г. Белгород, ул. Гражданский проспект 29а
						+e.tel(href="tel:+79440000000") +7 (944) 000-00-00
					.col-lg-3
						+e.listRow.row 
							.col-md-7
								+e.title УСЛУГИ
								ul 
									li: a(href="#")  Создание сайта
									li: a(href="#")  Дизайн и контент
									li: a(href="#")  Продвижение
									li: a(href="#")  Поддержка
							.col-md
								+e.title О нас
								ul 
									li: a(href="#")  Компания
									li: a(href="#")  Вакансии
									li: a(href="#")  Награды
					.col.text-center
						+e.btn(href="#") заполнить бриф
						+e.whrite или напишите нам
						+e.mail(href="mailto:BOX@DIGGET.ORG") BOX@DIGGET.ORG
					.col-lg-3
						+e.title МЫ В СОЦСЕТЯХ
						+soc()
		+e.foot 
			.container 
				.row.justify-content-between
					.col-sm-auto 
						+e.dev © 2012 – 2021, DIGGET Диджитал студия для Вашего бизнеса
					.col-sm-auto 
						+e.polite(href="#") Политика обработки персональных данных