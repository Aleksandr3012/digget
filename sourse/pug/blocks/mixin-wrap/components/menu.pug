//- menu
mixin mnu()

	+b.UL.navMenu(itemscope itemtype="http://schema.org/SiteNavigationElement")
		+e.LI.item.dropdown( itemprop="item" )
			+e.A.link(href='#'  itemprop="url") Услуги
			//- +b.Ul.drop
		+e.LI.item( itemprop="item" ): +e.A.link(href='#'  itemprop="url") Компания
		+e.LI.item( itemprop="item" ): +e.A.link(href='#'  itemprop="url") Контакты
//- /menu